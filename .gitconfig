[user]
	name = Marcin Bednarek
	email = paws77@gmail.com
[push]
	default = current
[credential]
	helper = store
[alias]
    st = status
    ci = commit -v
    aa = add -A
    ch-- = checkout --
    ch = checkout
[core]
	autocrlf = false
	excludesfile = /home/mbedna/.gitignore_global
	filemode = false
[color]
	diff = true
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path =
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[pull]
	default = current
[http]
	sslverify = false
[filesystem "Oracle Corporation|13|/dev/nvme0n1p6"]
	timestampResolution = 4000 nanoseconds
	minRacyThreshold = 6646 microseconds
[filesystem "Oracle Corporation|13.0.2|/dev/nvme0n1p6"]
	timestampResolution = 10000 nanoseconds
	minRacyThreshold = 6699 microseconds
