#paths that has to be changed
. $HOME/bin/env.sh 

alias rm="echo Use 'trash', or the full path i.e. '/bin/rm'"
export PROJECTS='/opt/projects'
export TOOLS='/opt/tools'
export DROPBOX='/opt/Dropbox'

alias godropbox='cd $DROPBOX'
alias gooracle='cd /opt/tools/OracleVM'
alias gotools='cd $TOOLS'
alias goprojects='cd $PROJECTS'
alias godocker='cd /var/lib/docker'

export MOST_BUSINESS_HOME="$PROJECTS/business"
export MOST_IMPORTER_HOME="$PROJECTS/importer"
export MOST_AGGREGATOR_HOME="$PROJECTS/aggregator"
export MOST_EXTERNAL_CONNECTOR_HOME="$PROJECTS/externalconnector"
export MOJAPG_HOME="$PROJECTS/mojapg/trunk"

alias gomostimporterbusiness='cd $MOST_IMPORTER_HOME/business'
alias gomostaggregatorbusiness='cd $MOST_AGGREGATOR_HOME/business'
alias gomostexternalconnectorbusiness='cd $MOST_EXTERNAL_CONNECTOR_HOME/business'
alias gomostbusiness='cd $MOST_BUSINESS_HOME/business'
alias gomostbusinessvagrant='cd $MOST_BUSINESS_HOME/vagrant'
alias gomostbusinessdata='cd $MOST_BUSINESS_HOME/vagrant/docker-run/most-business/data'
alias gomostimportervagrant='cd $MOST_IMPORTER_HOME/vagrant'
alias gomostimporterdata='cd $MOST_IMPORTER_HOME/vagrant/docker-run/most-importer/data'
alias gomostaggregatorvagrant='cd $MOST_AGGREGATOR_HOME/vagrant'
alias gomostaggregatordata='cd $MOST_AGGREGATOR_HOME/vagrant/docker-run/most-aggregator/data'
alias gomostexternalconnectorvagrant='cd $MOST_EXTERNAL_CONNECTOR_HOME/vagrant'
alias gomostexternalconnectordata='cd $MOST_EXTERNAL_CONNECTOR_HOME/vagrant/docker-run/most-externalconnector/data'

alias gomojapg='cd $MOJAPG_HOME'
alias gomojapgvagrant='cd $MOJAPG_HOME/vagrant'

alias startmostbusiness='docker start most-business'
alias restartmostbusiness='docker restart most-business'
alias startmostimporter='docker start most-importer'
alias restartmostimporter='docker restart most-importer'
alias startmostaggregator='docker start most-aggregator'
alias restartmostaggregator='docker restart most-aggregator'
alias startmostexternalconnector='docker start most-externalconnector'
alias restartmostexternalconnector='docker restart most-externalconnector'

alias startmojapg='cd $MOJAPG_HOME/vagrant;vagrant up'

alias gologsmostbusiness='cd $MOST_BUSINESS_HOME/vagrant/docker-run/most-business/logs' 
alias godeploymostbusiness='cd $MOST_BUSINESS_HOME/vagrant/docker-run/most-business/srv/webapp' 
alias taillogmostbusiness='tail -f $MOST_BUSINESS_HOME/vagrant/docker-run/most-business/logs/spring.log' 

alias gologsmostimporter='cd $MOST_IMPORTER_HOME/vagrant/docker-run/most-importer/logs' 
alias godeploymostimporter='cd $MOST_IMPORTER_HOME/vagrant/docker-run/most-importer/srv/webapp' 
alias taillogmostimporter='tail -f $MOST_IMPORTER_HOME/vagrant/docker-run/most-importer/logs/spring.log' 

alias gologsmostaggregator='cd $MOST_AGGREGATOR_HOME/vagrant/docker-run/most-aggregator/logs' 
alias godeploymostaggregator='cd $MOST_AGGREGATOR_HOME/vagrant/docker-run/most-aggregator/srv/webapp' 
alias taillogmostaggregator='tail -f $MOST_AGGREGATOR_HOME/vagrant/docker-run/most-aggregator/logs/spring.log' 

alias gologsmostexternalconnector='cd $MOST_EXTERNAL_CONNECTOR_HOME/vagrant/docker-run/most-externalconnector/logs' 
alias godeploymostexternalconnector='cd $MOST_EXTERNAL_CONNECTOR_HOME/vagrant/docker-run/most-externalconnector/srv/webapp' 
alias taillogmostexternalconnector='tail -f $MOST_EXTERNAL_CONNECTOR_HOME/vagrant/docker-run/most-externalconnector/logs/spring.log' 

alias taillogmojapgbusiness='tail -f $MOJAPG_HOME/vagrant/docker-run/mojapg-business-trunk/logs/log/server.log'
alias taillogmojapgportal='tail -f $MOJAPG_HOME/vagrant/docker-run/mojapg-portal-trunk/logs/log/server.log'
alias tailall='tail -f $MOJAPG_HOME/vagrant/docker-run/mojapg-portal-trunk/logs/log/server.log -f $MOJAPG_HOME/vagrant/docker-run/mojapg-business-trunk/logs/log/server.log -f $MOST_BUSINESS_HOME/vagrant/docker-run/most-business/logs/spring.log -f $MOST_IMPORTER_HOME/vagrant/docker-run/most-importer/logs/spring.log -f $MOST_EXTERNAL_CONNECTOR_HOME/vagrant/docker-run/most-externalconnector/logs/spring.log tail -f $MOST_AGGREGATOR_HOME/vagrant/docker-run/most-aggregator/logs/spring.log'

#alias goconf='cd $TOMCAT_HOME/conf' 
alias ll='ls -alF --color'
alias la='ls -A --color'
alias l='ls -CF --color'

if [[ $platform == 'linux' ]]; then
	export C_DRIVE='/media/sf_C_DRIVE'
	export ECLIM_ECLIPSE_HOME=$TOOLS/springsource/sts-2.9.1.RELEASE
	export ECLIPSE_HOME=$TOOLS/eclipse
	export IDEA_HOME=$TOOLS/idea-IU-183.4588.61
	export SOAPUI_HOME=$TOOLS/SoapUI-5.3.0
	export PATH=$PATH:$SOAPUI_HOME/bin:$IDEA_HOME/bin
	#looks like sqldeveloper is installed in two places TODO
	export SQLDEVELOPER_HOME=$TOOLS/sqldeveloper
	export PATH=$PATH:$SQLDEVELOPER_HOME/bin
	export M2_HOME=$TOOLS/apache-maven-3.6.0
    export GRADLE_HOME=$TOOLS/gradle-2.13
	export PATH=$PATH:$GRADLE_HOME/bin
#	export M2_HOME=/media/paws/PENDRIVE/programs/maven
	export ANT_HOME=$TOOLS/ant
	export JAVA_HOME=/usr/lib/jvm/java-12-openjdk-amd64
	export GANT_HOME=$TOOLS/gant-1.9.8
	alias starteclim=$ECLIM_ECLIPSE_HOME/eclimd
	alias starteclipseeclim=$ECLIM_ECLIPSE_HOME/STS
	alias starteclipse=$ECLIPSE_HOME/eclipse
	alias startidea=$IDEA_HOME/bin/idea.sh
	alias startsoapui=soapui.sh
	alias startsqldeveloper=sqldeveloper
    export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:~/repo/mbedna/oracle/instantclient_11_2
    export PATH=$PATH:~/repo/mbedna/oracle/instantclient_11_2
    export cl_path="$HOME/lib/javacompiler-1.0.jar"
elif [[ $platform == 'cygwin' ]]; then
	export C_DRIVE='/cygdrive/c'
	export D_DRIVE='/cygdrive/d'
	export E_DRIVE='/cygdrive/e'
    if [[ -z "$DRIVE" ]]; then
        export DRIVE=$C_DRIVE
    fi
	export ECLIMD_OPTS='-Dosgi.instance.area.default=C:/workspace'
	export ECLIPSE_HOME=$DRIVE/eclipse
	#Below is needed for eclim installation
	export ECLIM_ECLIPSE_HOME=$DRIVE/eclipse
	export M2_HOME=$TOOLS/maven
	export SBT_HOME=$DRIVE/programs/sbt
	export ANT_HOME=$TOOLS/ant
	export GANT_HOME=$DRIVE/programs/gant
	MOZILLA_HOME=$DRIVE/Program\ Files\ \(x86\)/Mozilla\ Firefox
	export JAVA_HOME=/usr/lib/jvm/java-12-openjdk-amd64
	export MONGODB_HOME=$DRIVE/programs/mongodb
	export SCALA_HOME=$DRIVE/scala
	export JBOSS_HOME=$DRIVE/jboss-6.1.0.Final
	alias starteclim='$ECLIPSE_HOME/eclimd.cmd'
	alias starteclipse=$ECLIPSE_HOME/eclipse.exe
    alias gotns='cd $DRIVE/programs/oracle/product/11.2.0/client_1/network/admin' 
	alias pbcopy='putclip'
	alias pbpaste='getclip'
    export cl_path=`cygpath -m $HOME/lib/javacompiler-1.0.jar`
    export CYGHOME=`cygpath -m $HOME`
    source ~/mintty-colors-solarized/mintty-blackburn.sh
    export VBOX_USER_HOME=/cygdrive/c/Users/mbednarek/.VirtualBox
elif [[ $platform == 'mac' ]]; then
    alias ll='ls -alF' 
    alias la='ls -A'
    alias l='ls -CF'
    JAVA_HOME=/Library/Java/JavaVirtualMachines/jdk1.8.0_40.jdk/Contents/Home
    [[ -s $(brew --prefix)/etc/profile.d/autojump.sh ]] && . $(brew --prefix)/etc/profile.d/autojump.sh
fi

ES_HOME=$TOOLS/elasticsearch-5.6.8
alias goidea='cd $IDEA_HOME'
alias goelastic='cd $ES_HOME'
alias startelastic=$ES_HOME/bin/elasticsearch
export TOMCAT_HOME=$C_DRIVE/apache-tomcat-7.0.29
